{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "space-before-function-paren": "off",
        "quotes": [
            "error",
            "double"
        ],
        "eol-last": "off",
        "max-len": [
            "error",
            {
                "code": 120
            }
        ]
    }
}